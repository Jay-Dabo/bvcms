@using UtilityExtensions
@{
    ViewBag.Title = "Giving Complete";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@if ((bool?)ViewBag.hasshell == true)
{
    <div class="no-header"></div>
}
else
{
    <h1 class="form-header">@ViewExtensions2.DbSetting("NameOfChurch", "Giving complete for : " + Util.Host)</h1>
}
<div class="row">
    <div class="col-lg-9">
        <div class="box box-responsive">

            <div class="box-content">
                Thank you, your giving was successful.
            </div>
        </div>
    </div>
</div>

